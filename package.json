{
  "name": "school-data-bridge",
  "version": "1.0.0",
  "description": "学校数据导入服务",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "worker": "RUN_MODE=worker ts-node src/index.ts",
    "scheduler": "RUN_MODE=scheduler ts-node src/index.ts",
    "produce": "RUN_MODE=producer ts-node src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bullmq": "^5.0.0",
    "dayjs": "^1.11.19",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "json5": "^2.2.3",
    "knex": "^3.1.0",
    "mysql2": "^3.9.2",
    "node-json-transform": "^1.1.2",
    "oracledb": "^6.10.0",
    "p-limit": "^3.1.0",
    "pg": "^8.16.3",
    "tedious": "^19.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
